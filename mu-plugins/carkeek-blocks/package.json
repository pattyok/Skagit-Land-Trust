{
    "name": "carkeek-blocks",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "checkFormat": "prettier --check \"**/*.{js,json,css,scss}\"",
        "format": "prettier --write \"**/*.{js,json,css,scss}\"",
        "lint": "eslint src",
        "start": "webpack --mode=development --watch",
        "build": "npm run lint && webpack --mode=production && gulp bundle",
        "packages-update": "wp-scripts packages-update",
        "scripts": "wp-scripts start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alialaa/gutenberg-course-webpack.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "linters": {
            "src/**/*.js": "eslint",
            "**/*.{js,json,css,scss}": [
                "prettier --write",
                "git add"
            ]
        }
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/alialaa/gutenberg-course-webpack/issues"
    },
    "homepage": "https://github.com/alialaa/gutenberg-course-webpack#readme",
    "dependencies": {
        "@wordpress/blob": "^2.9.0",
        "@wordpress/blocks": "^6.20.2",
        "@wordpress/compose": "^3.19.2",
        "@wordpress/data": "^4.22.2",
        "@wordpress/edit-post": "^3.21.2",
        "@wordpress/element": "^2.16.0",
        "@wordpress/html-entities": "^2.8.0",
        "@wordpress/i18n": "^3.14.0",
        "@wordpress/icons": "^2.7.0",
        "@wordpress/keycodes": "^2.15.0",
        "@wordpress/plugins": "^2.20.2",
        "jquery": "^3.5.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-slideshow-image": "^2.0.2",
        "react-sortable-hoc": "^1.11.0",
        "reactjs-popup": "^1.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@wordpress/browserslist-config": "^2.7.0",
        "@wordpress/components": "^10.0.2",
        "@wordpress/editor": "^9.20.2",
        "@wordpress/scripts": "^12.1.1",
        "autoprefixer": "^9.8.5",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "browserslist": "^4.13.0",
        "classnames": "^2.2.6",
        "clean-webpack-plugin": "^2.0.2",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
